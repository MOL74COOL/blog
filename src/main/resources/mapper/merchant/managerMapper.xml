<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.songtian.restaurant.mapper.merchant.managerMapper">

    <select id="selectById" resultType="com.songtian.restaurant.bean.merchant.manager">
		select* from manager where id=#{id}
	</select>

	<select id="selectUser" resultType="com.songtian.restaurant.bean.merchant.manager">
		select* from manager where id=#{id} and password=#{password}
	</select>
	<delete id="delete">
		DELETE FROM order1 WHERE 1=1
	</delete>

</mapper>